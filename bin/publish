#!/usr/bin/env node

var gh_pages = require('gh-pages');
var path = require('path');
var dist = path.join(__dirname, '..', 'dist');

var handle_error = function(error) {
  if (error) {
    console.error(error);
    return process.exit(error.code || 1);
  }
};


gh_pages.publish(dist, handle_error);
